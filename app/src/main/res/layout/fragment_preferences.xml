<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <RadioButton
        android:id="@+id/normalButton1"
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        android:backgroundTint="#FFFFFF"
        android:buttonTint="#FFFFFF"
        android:checked="false"
        android:fontFamily="@font/nobile_bold"
        android:shadowRadius="1.5"
        android:text="Normal"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/blackButton1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toEndOf="@+id/textView6" />

    <RadioButton
        android:id="@+id/blackButton1"
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        android:backgroundTint="#FFFFFF"
        android:buttonTint="#FFFFFF"
        android:checked="false"
        android:fontFamily="@font/nobile_bold"
        android:shadowRadius="1.5"
        android:text="Black"
        android:textColor="#FFFF"
        app:layout_constraintBottom_toTopOf="@+id/luxButton1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toEndOf="@+id/textView6" />

    <RadioButton
        android:id="@+id/luxButton1"
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:layout_marginBottom="492dp"
        android:layout_weight="1"
        android:backgroundTint="#ffffff"
        android:buttonTint="#ffffff"
        android:checked="false"
        android:fontFamily="@font/nobile_bold"
        android:shadowRadius="1.5"
        android:text="Luxury"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="164dp"
        android:layout_height="54dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/nobile_bold"
        android:text="Cab Type"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="154dp"
        android:layout_height="60dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/nobile_bold"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:layout_marginTop="140dp"
        android:text="Talkative"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="162dp"
        android:layout_height="60dp"
        android:layout_marginTop="44dp"
        android:text="Music Genre"
        android:fontFamily="@font/nobile_bold"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="159dp"
        android:layout_height="62dp"
        android:fontFamily="@font/nobile_bold"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:text="Only Favorites"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintVertical_bias="0.134" />

    <CheckBox
        android:id="@+id/checkBox1"
        android:layout_width="67dp"
        android:layout_height="45dp"
        android:layout_marginBottom="52dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        app:layout_constraintBottom_toTopOf="@+id/spinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.305"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="186dp"
        android:layout_height="59dp"
        android:layout_marginBottom="52dp"
        app:layout_constraintBottom_toTopOf="@+id/checkBox2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toEndOf="@+id/textView8" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="67dp"
        android:layout_height="45dp"
        android:layout_marginBottom="204dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toEndOf="@+id/textView9" />
</androidx.constraintlayout.widget.ConstraintLayout>